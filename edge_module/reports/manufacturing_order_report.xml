<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Report template -->
        <template id="report_manufacturing_order">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <t t-foreach="docs" t-as="doc">
                        <div class="page">
                            <!-- Display manufacturing order details -->
                            <h2>Manufacturing Order: <span t-field="doc.name"/></h2>
                            <p>Product: <span t-field="doc.product_id"/></p>
                            <p>Quantity: <span t-field="doc.product_qty"/></p>
                            <!-- Add more fields as needed -->
                        </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>