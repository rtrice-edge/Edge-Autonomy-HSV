<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
        <record id="inherit_purchase_purchase_order_form" model="ir.ui.view">
            <field name="name">inherit.purchase.order.line.form.view</field>
            <field name="model">purchase.order.line</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="/form/sheet/notebook/page[1]/field[@name='order_line']/tree/field[@name='name']" position="after">
                    <field name="costobjective"/>
                    <field name="expensetype"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
