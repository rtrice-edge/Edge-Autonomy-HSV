<odoo>
  <!-- Search View -->
  <record id="view_mo_time_quality_report_search" model="ir.ui.view">
    <field name="name">mo.time.quality.report.search</field>
    <field name="model">mo.time.quality.report</field>
    <field name="arch" type="xml">
      <search string="MO Time &amp; Quality Report">
        <!-- keep your dept filter -->
        <filter string="Electronics &amp; Quality"
                domain="[('department','in',['Electronics','Quality'])]"
                default="1"/>

        <field name="date_start_min"
                string="Start After"
                filter_domain="[('date_start_min','&gt;=', self)]"/>
         <field name="date_end_max"
                string="End Before"
                filter_domain="[('date_end_max','&lt;=', self)]"/>
      </search>
    </field>
  </record>

  <!-- Tree View: just show your new date columns up front -->
  <record id="view_mo_time_quality_report_tree" model="ir.ui.view">
    <field name="name">mo.time.quality.report.tree</field>
    <field name="model">mo.time.quality.report</field>
    <field name="arch" type="xml">
      <tree string="MO Time &amp; Quality Report">
        <field name="date_start_min"/>
        <field name="date_end_max"/>
        <field name="product_code"/>
        <field name="product_name"/>
        <!-- … the rest of your fields … -->
      </tree>
    </field>
  </record>

  <!-- Pivot View: you can include date_start_min / date_end_max as rows if you like -->
  <record id="view_mo_time_quality_report_pivot" model="ir.ui.view">
    <field name="name">mo.time.quality.report.pivot</field>
    <field name="model">mo.time.quality.report</field>
    <field name="arch" type="xml">
      <pivot string="MO Time &amp; Quality Analysis">
        <field name="date_start_min"   type="row"/>
        <field name="date_end_max"     type="row"/>
        <field name="product_code"     type="row"/>
        <!-- … the rest of your rows / cols … -->
      </pivot>
    </field>
  </record>

  <!-- Action + Menu: make sure you include the search view -->
  <record id="action_mo_time_quality_report" model="ir.actions.act_window">
    <field name="name">MO Time &amp; Quality Report</field>
    <field name="res_model">mo.time.quality.report</field>
    <field name="view_mode">pivot,tree,search</field>
    <field name="search_view_id" ref="view_mo_time_quality_report_search"/>
  </record>
  <menuitem id="menu_mo_time_quality_report"
            name="MO Time &amp; Quality"
            parent="mrp.menu_mrp_reporting"
            action="action_mo_time_quality_report"
            sequence="20"/>
</odoo>
